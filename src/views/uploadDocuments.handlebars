 <div id="content" data-uid={{id}}>
    <div class="container mt-2">
      <div class="row">
        <div class="col-lg-12 ">
          <!-- breadcrumb-->
          <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
              <li class="breadcrumb-item"><a href="/">Home</a></li>
              <li aria-current="page" class="breadcrumb-item active">Upload Documents</li>
            </ol>
          </nav>
        </div>
        <div class="col-lg-6 mx-auto d-flex justify-content-center">
          <div class="box">
            <h1>Upload Documents</h1>
            <hr>
            <form id="upload-form" action="#" method="post" enctype="multipart/form-data" onsubmit="uploadDocuments(event)">
              <div class="form-group">
                <label for="documents">Select Documents</label>
                <input id="documents" type="file" class="form-control" multiple>
              </div>
              <div class="text-center">
                <button type="submit" class="btn btn-primary mt-2" id="uploadbtn"><i class="fa fa-upload"></i> Upload</button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <script src="/js/updloadDocuments.js"></script>
